{{#reveal-slide class="lsgdd-FreestyleSlide lsgdd-ObjectivesSlide"}}
  {{presentations/slide-header
    super="Ruthlessly Efficient Exposition"
    main="Rebranding Betterment Responsively"
    sub=""
  }}
  {{#presentations/slide-body columns=4 class="u-beforeAfter"}}
    {{#presentations/slide-column}}
      <div>
        <div class="u-smallHeader">March</div>
        <div class="lsgdd-TightContainer">
          <img class="lsgdd-Image u-image--l0 u-shadow" src="assets/images/example-before-mobile-summary.png" alt="Before">
        </div>
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div>
        <div class="u-smallHeader">&nbsp;</div>
        <div class="lsgdd-TightContainer">
          <img class="lsgdd-Image u-image--l0 u-shadow" src="assets/images/example-before-mobile-performance.png" alt="Before">
        </div>
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div class="fragment" data-fragment-index="1">
        <div class="u-smallHeader">May</div>
        <div class="fade-in">
          <div class="lsgdd-TightContainer">
            <img class="lsgdd-Image u-image--l0 u-shadow" src="assets/images/example-after-mobile-summary.jpg" alt="After">
          </div>
        </div>
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div class="fragment" data-fragment-index="1">
        <div class="u-smallHeader">&nbsp;</div>
        <div class="fade-in">
          <div class="lsgdd-TightContainer">
            <img class="lsgdd-Image u-image--l0 u-shadow" src="assets/images/example-after-mobile-performance.jpg" alt="After">
          </div>
        </div>
      </div>
    {{/presentations/slide-column}}
  {{/presentations/slide-body}}
  {{#presentations/slide-footer}}
    <div class="lsgdd-TightContainer lsgdd-ObjectivesSlide-promiseContainer">
      <img class="lsgdd-Image u-rounded u-shadow u-image--s" src="assets/images/uh-oh.jpg" alt="Uh oh">
    </div>
  {{/presentations/slide-footer}}
{{/reveal-slide}}
