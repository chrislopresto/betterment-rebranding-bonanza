{{#reveal-slide class="lsgdd-FreestyleSlide lsgdd-CssForaysSlide"}}
  {{presentations/slide-header
    super=""
    main="Specificity Wars"
    sub="CSS Forays"
  }}
  {{#presentations/slide-body columns=2}}
    {{#presentations/slide-column}}
      <div class="lsgdd-TightContainer lsgdd-CssForaysSlide-imageContainer">
        <img class="lsgdd-Image lsgdd-CssForaysSlide-image lsgdd-image--boxShadow" src="assets/images/css-forays-specificity.png" alt="Specificity">
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div class="lsgdd-TightContainer lsgdd-CssForaysSlide-imageContainer fragment">
        <img class="lsgdd-Image u-grayscale lsgdd-CssForaysSlide-image lsgdd-image--boxShadow" src="assets/images/css-toddlers.jpg" alt="Toddlers">
      </div>
    {{/presentations/slide-column}}
  {{/presentations/slide-body}}
  {{#presentations/slide-footer}}
  {{/presentations/slide-footer}}
  {{#reveal-slide-notes}}
  {{/reveal-slide-notes}}
{{/reveal-slide}}
