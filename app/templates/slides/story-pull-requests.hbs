{{#reveal-slide class="lsgdd-FreestyleSlide lsgdd-ObjectivesSlide"}}
  {{presentations/slide-header
    super=""
    main="Pull Requests"
    sub="Stats and stories"
  }}
  {{#presentations/slide-body columns=3}}
    {{#presentations/slide-column}}
      <div class="u-smallHeader">Style Closet</div>
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-image--boxShadow lsgdd-image--maxHeight630" src="assets/images/commits-style-closet.gif" alt="Pull requests">
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div class="u-smallHeader">Brochure</div>
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-image--boxShadow lsgdd-image--maxHeight630" src="assets/images/commits-brochure.gif" alt="Pull requests">
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div class="u-smallHeader">Core repository - includes Retail</div>
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-image--boxShadow lsgdd-image--maxHeight630" src="assets/images/commits-better-core.gif" alt="Pull requests">
      </div>
    {{/presentations/slide-column}}
  {{/presentations/slide-body}}
  {{#presentations/slide-footer}}

  {{/presentations/slide-footer}}
  {{#reveal-slide-notes}}
  {{/reveal-slide-notes}}
{{/reveal-slide}}
