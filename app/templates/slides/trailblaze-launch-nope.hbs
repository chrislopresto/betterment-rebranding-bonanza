{{#reveal-slide class="lsgdd-FreestyleSlide lsgdd-ObjectivesSlide"}}
  {{presentations/slide-header
    super="Trailblazing"
    main="Aim For Stable Markup"
    sub=""
  }}
  {{#presentations/slide-body columns=3 align="left"}}
    {{#presentations/slide-column class="SlideColumn--paddedLeft"}}
      <div class="u-smallHeader">Before</div>
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-image--boxShadow lsgdd-image--maxHeight630" src="assets/images/trailblaze-markup-before.png" alt="Maybe not...">
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column class="SlideColumn--paddedLeft SlideColumn--paddedRight"}}
      <div class="u-smallHeader">After</div>
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-image--boxShadow lsgdd-image--maxHeight630" src="assets/images/trailblaze-markup-after.png" alt="Maybe not...">
      </div>
    {{/presentations/slide-column}}
    {{#presentations/slide-column class="SlideColumn--paddedLeft"}}
      <div class="u-smallHeader">Stress Test</div>
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-image--boxShadow lsgdd-image--maxHeight630" src="assets/images/trailblaze-markup.gif" alt="Maybe not...">
      </div>
    {{/presentations/slide-column}}
  {{/presentations/slide-body}}
{{/reveal-slide}}
