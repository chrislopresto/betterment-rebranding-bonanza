{{#reveal-slide class="lsgdd-FreestyleSlide lsgdd-ObjectivesSlide"}}
  {{presentations/slide-header
    super=""
    main="TestTrack"
    sub="Rebranding Prerequisites"
  }}
  {{#presentations/slide-body columns=2}}
    {{#presentations/slide-column class="SlideColumn--paddedRight"}}
      {{#presentations/slide-list}}
        {{#presentations/slide-list-item type="borderless" class="SlideListItem--withSubItems"}}
          <a href="https://github.com/Betterment/test_track">github.com/Betterment/test_track</a>
        {{/presentations/slide-list-item}}
        {{#presentations/slide-list-item type="sub"}}
          <ul>
            <li>Feature flag and split testing framework</li>
            <li>Consistent experiences across devices</li>
            <li>Rails, JS, Objective C, &amp; Java clients</li>
            <li>Chrome extension to override assignments</li>
          </ul>
        {{/presentations/slide-list-item}}
      {{/presentations/slide-list}}
    {{/presentations/slide-column}}
    {{#presentations/slide-column}}
      <div class="lsgdd-TightContainer">
        <img class="lsgdd-Image lsgdd-ObjectivesSlide-promise" src="assets/images/test-track.jpg" alt="When launching a design system, it helps to already have one.">
      </div>
    {{/presentations/slide-column}}
  {{/presentations/slide-body}}
  {{#presentations/slide-footer}}
  {{/presentations/slide-footer}}
  {{#reveal-slide-notes}}
  {{/reveal-slide-notes}}
{{/reveal-slide}}
